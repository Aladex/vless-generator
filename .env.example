# VLESS Server Configuration
VLESS_SERVER=vless.example.com
VLESS_PORT=443
WS_PATH=/websocket

# Network Configuration
DNS_SERVER=8.8.8.8
DOH_SERVER=https://223.5.5.5/dns-query
